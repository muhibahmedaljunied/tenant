{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.2",
        "aloha/twilio": "4.0.4",
        "arcanedev/log-viewer": "*",
        "automattic/woocommerce": "*",
        "balping/json-raw-encoder": "^1.0",
        "barryvdh/laravel-dompdf": "*",
        "cmgmyr/messenger": "^2.30",
        "composer/semver": "*",
        "doctrine/dbal": "^3.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "itsnasser/egyptian-e-invoice": "dev-main",
        "knox/pesapal": "*",
        "laravel/framework": "^11.0",
        "laravel/passport": "^12.0",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.5",
        "laravel/ui": "*",
        "laravel-notification-channels/fcm": "^4.3",
        "larswiegers/laravel-translations-checker": "^0.9",
        "livewire/livewire": "^3.4",
        "maatwebsite/excel": "^3.0",
        "milon/barcode": "^12.0",
        "mpdf/mpdf": "^8.2.3",
        "nwidart/laravel-modules": "^8.3.0",
        "owen-it/laravel-auditing": "^14.0",
        "pusher/pusher-php-server": "^7.0",
        "ramsey/uuid": "^4.1",
        "razorpay/razorpay": "*",
        "spatie/flysystem-dropbox": "*",
        "spatie/laravel-activitylog": "^4.10",
        "spatie/laravel-backup": "^9.0.0",
        "spatie/laravel-html": "^3.12",
        "spatie/laravel-permission": "^6.18",
        "srmklive/paypal": "*",
        "stancl/tenancy": "3.8",
        "stripe/stripe-php": "*",
        "unicodeveloper/laravel-paystack": "*",
        "yajra/laravel-datatables": "^11",
        "yajra/laravel-datatables-export": "^11",
        "yajra/laravel-datatables-oracle": "^v11.0.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "barryvdh/laravel-ide-helper": "*",
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^8.0.0",
        "phpunit/phpunit": "^11.0.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "files": [
            "app/Http/helpers.php"
        ],
        "classmap": [
            "database",
            "database/factories",
            "database/seeders"
        ],
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    }
}
